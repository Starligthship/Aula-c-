#include <stdio.h>

int main() {
    int x = 11;
    int *ptr = &x;

    printf("Valor de x: %d\n", x);
    printf("Endereço de x: %p\n", (void*) &x);
    printf("Conteúdo do ponteiro ptr: %p\n", (void*) ptr);
    printf("Valor apontado por ptr: %d\n", *ptr);
    printf("valor do ponteiro de x: %p\n", (void*) x);
    return 0;
}
